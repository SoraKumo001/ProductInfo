{
  "name": "product-info",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --preserve-symlinks dist/app/index.js",
    "test": "node --preserve-symlinks dist/app/index.js --test",
    "watch:test": "concurrently \"npm:watch:front\" \"npm:watch:back\" \"npm:test\"",
    "watch:all": "concurrently \"npm:watch:front\" \"npm:watch:back\" \"npm:start\"",
    "build:all": "concurrently \"npm run build:back\" \"npm run build:front-module && npm run build:front\"",
    "build:front-module": "concurrently \"npm:build:react\" \"npm:build:redux-module\"",
    "build:redux-module": "tsc -b node_modules/@jswf/redux-module/src/",
    "watch:redux-module": "tsc -b -w node_modules/@jswf/redux-module/src/",
    "build:react": "tsc -b node_modules/@jswf/react/src/",
    "watch:react": "tsc -b -w node_modules/@jswf/react/src/",
    "watch:front": "webpack -b src/front/webpack.config.js -w --mode development",
    "build:front": "webpack --config src/front/webpack.config.js",
    "watch:back": "tsc -b -w ",
    "build:back": "tsc -b "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
